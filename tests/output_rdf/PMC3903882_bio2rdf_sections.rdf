                                                  <rdf:Description rdf:about="http://bio2rdf.org/pmc_resource:3903882_reference_pone.0087440-Matin1">
                                                    <sio:SIO_000278 rdf:resource="http://bio2rdf.org/pmc_resource:3903882_paragraph_Discussion_para_1"/>
                                                  </rdf:Description>
