                                    <sio:SIO_000028 rdf:resource="http://bio2rdf.org/pmc_resource:1183526_paragraph_Results_Prevention-of-Ataxia-and-Weight-Loss-in-Chimeric-Mice_para_1"/>
                                    <sio:SIO_000068 rdf:resource="http://bio2rdf.org/pmc_resource:1183526_section_Results"/>
                                    <sio:SIO_000008>
                                      <sio:SIO_000175 rdf:about="http://bio2rdf.org/pmc_resource:1183526_other_dcterms_title_node99_1495126521648">
