                                                          <rdf:Description rdf:about="http://bio2rdf.org/pmc_resource:1183527_reference_pgen-0010008-b06">
                                                            <sio:SIO_000278 rdf:resource="http://bio2rdf.org/pmc_resource:1183527_paragraph_Introduction_para_2"/>
                                                          </rdf:Description>
