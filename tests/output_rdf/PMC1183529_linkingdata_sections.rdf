      <bibo:Document rdf:about="http://linkingdata.io/reference/pmcdoc_resource/1183529/pgen-0010010-b41">
        <bibo:citedBy rdf:resource="http://linkingdata.io/paragraph/pmcdoc_resource/1183529/-Discussion:para_3"/>
      </bibo:Document>
    </bibo:cites>
    <dcterms:isPartOf rdf:resource="http://linkingdata.io/section/pmcdoc_resource/1183529/-Discussion"/>
