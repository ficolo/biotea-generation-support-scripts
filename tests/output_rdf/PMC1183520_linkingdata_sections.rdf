                                                        <dcterms:references rdf:resource="http://206.167.190.233/candida/index.cfm?page=CaChrom"/>
                                                        <dcterms:references rdf:resource="http://agabian.ucsf.edu/canoDB/anno.php"/>
                                                        <dcterms:isPartOf rdf:resource="http://linkingdata.io/section/pmcdoc_resource/1183520/Materials-and-Methods_no-title-1_Identification-of-C--albicans-ORFs-and-merging-of-preliminary-annotations-"/>
                                                      </doco:Paragraph>
                                                    </bibo:citedBy>
                                                    <bibo:citedBy rdf:resource="http://linkingdata.io/paragraph/pmcdoc_resource/1183520/Results-Discussion_The-Annotation-Process_Compilation-of-Candida-annotation-data-:para_2"/>
