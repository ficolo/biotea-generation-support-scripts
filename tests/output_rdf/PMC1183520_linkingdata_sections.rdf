                                              <bibo:Document rdf:about="http://linkingdata.io/reference/pmcdoc_resource/1183520/pgen-0010001-b74">
                                                <bibo:citedBy rdf:resource="http://linkingdata.io/paragraph/pmcdoc_resource/1183520/Results-Discussion_Multigene-Families_Zinc-cluster-transcription-factors-:para_2"/>
                                              </bibo:Document>
                                            </bibo:cites>
