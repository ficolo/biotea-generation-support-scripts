                <bibo:citedBy rdf:resource="http://linkingdata.io/paragraph/pmcdoc_resource/1183520/Materials-and-Methods_no-title-1_Identification-of-spurious-genes-:para_1"/>
              </bibo:Document>
            </bibo:cites>
