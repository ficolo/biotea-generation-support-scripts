                                                    <bibo:citedBy rdf:resource="http://linkingdata.io/paragraph/pmcdoc_resource/1183520/Results-Discussion_The-Annotation-Process_Compilation-of-Candida-annotation-data-:para_2"/>
                                                  </bibo:Document>
