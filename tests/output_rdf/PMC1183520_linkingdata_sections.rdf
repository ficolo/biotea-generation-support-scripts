                                    <dcterms:title>A Human-Curated Annotation of the Candida albicans Genome</dcterms:title>
                                    <dcterms:references rdf:resource="http://www-genome.wi.mit.edu/annotation/fungi/magnaporthe/"/>
                                    <dcterms:references rdf:resource="http://genolist.pasteur.fr/CandidaDB"/>
