                                                  <bibo:Document rdf:about="http://linkingdata.io/reference/pmcdoc_resource/1183523/pgen-0010004-b62">
                                                    <bibo:citedBy rdf:resource="http://linkingdata.io/paragraph/pmcdoc_resource/1183523/Discussion_Alternative-Glaucoma-Hypotheses:para_1"/>
                                                  </bibo:Document>
