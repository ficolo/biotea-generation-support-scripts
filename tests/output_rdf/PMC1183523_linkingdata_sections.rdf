                        <bibo:cites rdf:resource="http://linkingdata.io/reference/pmcdoc_resource/1183523/pgen-0010004-b25"/>
                        <bibo:cites>
                          <bibo:Document rdf:about="http://linkingdata.io/reference/pmcdoc_resource/1183523/pgen-0010004-b38">
                            <bibo:citedBy rdf:resource="http://linkingdata.io/paragraph/pmcdoc_resource/1183523/Introduction:para_3"/>
                          </bibo:Document>
