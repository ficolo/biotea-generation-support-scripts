                            </bibo:cites>
                            <bibo:cites>
                              <bibo:Document rdf:about="http://linkingdata.io/reference/pmcdoc_resource/1183523/pgen-0010004-b51">
                                <bibo:citedBy rdf:resource="http://linkingdata.io/paragraph/pmcdoc_resource/1183523/Results_Homozygous-BAX-Deficiency-Alters-IOP:para_4"/>
