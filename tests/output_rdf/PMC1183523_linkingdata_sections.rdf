                                      <doco:Paragraph rdf:about="http://linkingdata.io/paragraph/pmcdoc_resource/1183523/Introduction:para_1">
                                        <bibo:cites>
                                          <bibo:Document rdf:about="http://linkingdata.io/reference/pmcdoc_resource/1183523/pgen-0010004-b06">
                                            <bibo:citedBy rdf:resource="http://linkingdata.io/paragraph/pmcdoc_resource/1183523/Introduction:para_1"/>
                                          </bibo:Document>
