                              <rdf:Seq rdf:about="http://linkingdata.io/listOfParagraphs/pmcdoc_resource/1183528/list8">
                                <rdf:li rdf:resource="http://linkingdata.io/paragraph/pmcdoc_resource/1183528/Results_-rhino-Evolution-in-Other-Drosophila-Species:para_1"/>
                                <rdf:li rdf:resource="http://linkingdata.io/paragraph/pmcdoc_resource/1183528/Results_-rhino-Evolution-in-Other-Drosophila-Species:para_2"/>
