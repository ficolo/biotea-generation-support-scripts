                                              <bibo:Document rdf:about="http://linkingdata.io/reference/pmcdoc_resource/1183528/pgen-0010009-b22">
                                                <bibo:citedBy>
                                                  <doco:Paragraph rdf:about="http://linkingdata.io/paragraph/pmcdoc_resource/1183528/Introduction:para_4">
                                                    <bibo:cites rdf:resource="http://linkingdata.io/reference/pmcdoc_resource/1183528/pgen-0010009-b35"/>
