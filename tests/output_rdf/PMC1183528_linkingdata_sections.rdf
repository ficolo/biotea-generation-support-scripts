                              </doco:Paragraph>
                            </dcterms:hasPart>
                            <dcterms:hasPart rdf:resource="http://linkingdata.io/paragraph/pmcdoc_resource/1183528/Results_Positive-Selection-of-rhino-Chromo-Domain:para_3"/>
                            <dcterms:hasPart rdf:resource="http://linkingdata.io/paragraph/pmcdoc_resource/1183528/Results_Positive-Selection-of-rhino-Chromo-Domain:para_2"/>
                            <dcterms:hasPart rdf:resource="http://linkingdata.io/paragraph/pmcdoc_resource/1183528/Results_Positive-Selection-of-rhino-Chromo-Domain:para_1"/>
                            <biotea:paragraphList>
                              <rdf:Seq rdf:about="http://linkingdata.io/listOfParagraphs/pmcdoc_resource/1183528/list9">
                                <rdf:li rdf:resource="http://linkingdata.io/paragraph/pmcdoc_resource/1183528/Results_Positive-Selection-of-rhino-Chromo-Domain:para_1"/>
                                <rdf:li rdf:resource="http://linkingdata.io/paragraph/pmcdoc_resource/1183528/Results_Positive-Selection-of-rhino-Chromo-Domain:para_2"/>
                                <rdf:li rdf:resource="http://linkingdata.io/paragraph/pmcdoc_resource/1183528/Results_Positive-Selection-of-rhino-Chromo-Domain:para_3"/>
