                                  <bibo:Document rdf:about="http://linkingdata.io/reference/pmcdoc_resource/1183528/pgen-0010009-b72">
                                    <bibo:citedBy rdf:resource="http://linkingdata.io/paragraph/pmcdoc_resource/1183528/Materials-and-Methods_no-title-1_Sequences-from-Drosophila-and-databases-and-RT-PCR-:para_1"/>
                                  </bibo:Document>
                                </bibo:cites>
                                <bibo:cites>
                                  <bibo:Document rdf:about="http://linkingdata.io/reference/pmcdoc_resource/1183528/pgen-0010009-st002">
                                    <bibo:citedBy rdf:resource="http://linkingdata.io/paragraph/pmcdoc_resource/1183528/Materials-and-Methods_no-title-1_Sequences-from-Drosophila-and-databases-and-RT-PCR-:para_1"/>
                                  </bibo:Document>
