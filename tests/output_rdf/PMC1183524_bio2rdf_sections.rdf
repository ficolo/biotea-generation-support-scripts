                                                              <rdf:Description rdf:about="http://bio2rdf.org/pmc_resource:1183524_reference_pgen-0010005-b03">
    <sio:SIO_000278 rdf:resource="http://bio2rdf.org/pmc_resource:1183524_paragraph_Introduction_para_3"/>
    <sio:SIO_000278>
      <sio:SIO_000110 rdf:about="http://bio2rdf.org/pmc_resource:1183524_paragraph_Introduction_para_2">
        <sio:SIO_000277>
          <rdf:Description rdf:about="http://bio2rdf.org/pmc_resource:1183524_reference_pgen-0010005-b05">
            <sio:SIO_000278 rdf:resource="http://bio2rdf.org/pmc_resource:1183524_paragraph_Introduction_para_2"/>
          </rdf:Description>
        </sio:SIO_000277>
        <sio:SIO_000277>
          <rdf:Description rdf:about="http://bio2rdf.org/pmc_resource:1183524_reference_pgen-0010005-b11">
            <sio:SIO_000278 rdf:resource="http://bio2rdf.org/pmc_resource:1183524_paragraph_Introduction_para_2"/>
